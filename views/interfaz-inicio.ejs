            <div class="frame">
                <div class="nav">
                    <ul class="links">
                        <li class="signin-active"><a class="btn">Sign in</a></li>
                        <li class="signup-inactive"><a class="btn">Sign up </a></li>
                    </ul>
                </div>
                <div ng-app ng-init="checked = false">
                    <form class="form-signin" action="/login" method="post" name="form">
                        <label for="correo">Username or email</label>
                        <input class="form-styling" type="text" name="correo" placeholder="" />
                        <label for="contrasenia" class="form-label">Password</label>
                        <input name="contrasenia" type="password" class="form-styling form-control"
                        id="contrasenia">
                        <label for="checkbox"><span class="ui"></span>Keep me signed in</label>
                        <button type="submit" class="btn btn-primary btn-animate btn-signin">Sign in</button>
                    </form>
    
                    <form class="form-signup" action="/interfaz-inicio" method="post" name="form">
                        <label for="exampleInputPassword1" class="form-label">Full name</label>
                        <input class="form-styling form-control" type="text" name="name" placeholder=""  id="exampleInputPassword1"/>
                        <label for="exampleInputEmail1" class="form-label">Email</label>
                        <input class="form-styling form-control" type="text" name="email" placeholder="" id="exampleInputEmail1"/>

                        <label for="exampleInputPassword1" class="form-label">user name</label>
                        <input class="form-styling form-control" type="text" name="username" placeholder=""  id="exampleInputPassword1"/>

                        <label for="exampleInputPassword1" class="form-label">Password</label>
                        <input class="form-styling form-control" type="password" name="password" placeholder="" />
                        <label for="exampleInputPassword1">Confirm password</label>
                        <input class="form-styling form-control" type="password" name="passwordTwo" placeholder="" />
                        <button ng-click="checked = !checked" type="submit" class="btn-signup btn btn-primary">Sign Up</button>
                        <!-- <button type="submit" class="btn btn-primary">Submit</button> -->
                    </form>
    
                    <div class="success">
