        <div class="derecha">
            <div class="main-home">
                <div class="header">
                    <div class="search">
                        <i class="ri-search-line"></i>
                        <input type="text" placeholder="search">
                    </div>
                </div>
            </div>
        
            <script src="https://cdn.jsdelivr.net/npm/chance"></script>
            <script>
                function generarPublicacion(i) {
                    let chance = new Chance();
                    let nombreUsuario = chance.name();
                    let contenido = chance.paragraph();
                    let likes = chance.integer({ min: 0, max: 100 });
                    let comentarios = chance.integer({ min: 0, max: 200 });
                    let compartidos = chance.integer({ min: 0, max: 1000 });
                    let descargas = chance.integer({ min: 0, max: 90000 });

                    let publicacion = `
                        <div class="publicacion">
                            <div class="nombre-usuario">
                                <div class="post-profile">
                                        <div class="post-img">
                                            <img src="img/4.jpeg" alt="">
                                        </div>
                                        <h3>${nombreUsuario}</h3>
                                    </div>    
                            </div>
                            <div class="contenido">${contenido}</div>
                            <img src="${i}" alt="Imagen de la publicaciÃ³n" class="imagen-publicacion">
        
                            <div class="post-box">
                                <div>
                                    <i class="ri-heart-line"></i>
                                    <span>${likes}k</span>
                                </div>
                                <div>
                                    <i class="ri-chat-3-line"></i>
                                    <span>${comentarios}k</span>
                                </div>  
                                <div>
                                    <i class="ri-download-cloud-2-line"></i>
                                    <span>${descargas}k</span>
                                </div>
                            </div>
                        </div>
                        <div class="horizontal-line"></div>
                    `;
                    return publicacion;
                }
        
                function numeros() {
                    let imagenes = [
                        'data:image/jpeg;base64,',
                        'https://previews.123rf.com/images/grekov/grekov1605/grekov160500169/58371175-la-familia-de-cuatro-personas-que-buscan-hermoso-paisaje-marino-en-las-monta%C3%B1as.jpg',
                        'https://thumbs.dreamstime.com/z/gafas-de-cerveza-y-paisajes-blancos-invierno-nevados-con-techo-mesa-para-productos-decoraciones-sensaci%C3%B3n-nevado-espacio-sus-161111035.jpg',
                        'https://ep01.epimg.net/elpais/imagenes/2019/10/30/album/1572424649_614672_1572453030_noticia_normal.jpg',
                        'https://images.unsplash.com/reserve/Af0sF2OS5S5gatqrKzVP_Silhoutte.jpg?ixlib=rb-4.0.3&ixid=M3wxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8fA%3D%3D&auto=format&fit=crop&w=870&q=80',
                        'https://www.blogdelfotografo.com/wp-content/uploads/2020/04/fotografo-paisajes-1536x832.webp',
                        'https://cdn.pixabay.com/photo/2017/01/23/19/40/woman-2003647_960_720.jpg',
                        'https://cdn.pixabay.com/photo/2016/07/22/09/59/fruits-1534494_640.jpg'
                    ];
                    return imagenes;
                }
        
                window.onload = function() {
                    let imagenes = numeros();
                    let container = document.getElementById('publicacion');
                    let publicaciones = ``;
                    for (let i = 0; i < imagenes.length; i++) {
                        let publicacion = generarPublicacion(imagenes[i]);
                        container.innerHTML += publicacion;
                        publicaciones += publicacion;
                    }
                };
            </script>
            <div class="publicaciones" id="publicacion"></div>
        </div>


        
        <div class="container">
